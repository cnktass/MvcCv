@using MvcCv.Models.Entity
@model List<TblYeteneklerim>


<div class="resume-section-content">
    <h2 class="mb-5">Yetenekler</h2>
    <ul class="list-inline dev-icons">
        <li class="list-inline-item"><i class="fab fa-html5"></i></li>
        <li class="list-inline-item"><i class="fab fa-css3-alt"></i></li>
        <li class="list-inline-item"><i class="fas fa-database"></i></li>
        <li class="list-inline-item"><i class="fas fa-project-diagram"></i></li>
     


    </ul>
    <div class="subheading mb-3">Beceriler</div>
    <ul class="fa-ul mb-0">
        @foreach(var item in Model)
        {

        <li>
            <span class="fa-li"><i class="fas fa-check"></i></span>
            @item.Yetenek
        </li>

        }
    </ul>
</div>